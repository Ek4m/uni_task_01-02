<%- include("../partials/metas")%> <%- include("../partials/header")%>
<main>
  <form action="/wagons/add" method="post">
    <h1>Add Wagon Type</h1>
    <div class="form-group">
      <label for="name">name</label>
      <input type="text" required id="name" name="name" />
    </div>
    <div class="form-group">
      <label for="duration">duration</label>
      <input type="date" required id="duration" name="duration" />
    </div>
    <div class="form-group">
      <label for="duration">Wagon Type</label>
      <select required name="wagonType">
        <%wagonTypes.length && wagonTypes.forEach(elem => {%>
        <option value="<%=elem.id%>"><%=elem.name%></option>
        <%})%>
      </select>
    </div>
    <button id="submit">Submit</button>
  </form>
</main>
<%- include("../partials/footer")%>
