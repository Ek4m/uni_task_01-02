<%- include("../partials/metas")%> <%- include("../partials/header")%>
<main>
  <form action="/warehouses/add" method="post">
    <h1>Add new warehouse</h1>
    <div class="form-group">
      <label for="name">name</label>
      <input type="text" required id="name" name="name" />
    </div>
    <div class="form-group">
      <label for="location">location</label>
      <input type="text" required id="location" name="location" />
    </div>
    <div class="form-group">
      <label for="capacity">capacity</label>
      <input type="number" required id="capacity" name="capacity" />
    </div>
    <div class="form-group">
      <label for="opensIn">opens at</label>
      <input type="time" step="1" required id="opensIn" name="opensIn" />
    </div>
    <div class="form-group">
      <label for="closesIn">closes at</label>
      <input type="time" step="1" required id="closesIn" name="closesIn" />
    </div>
    <div class="form-group">
      <label for="price">price</label>
      <input type="number" required id="price" name="price" />
    </div>

    <button id="submit">Submit</button>
  </form>
</main>
<%- include("../partials/footer")%>
